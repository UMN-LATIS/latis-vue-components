{
  "name": "@umn-latis/latis-vue-components",
  "description": "A collection vue components for latis projects. Very incomplete and experimental.",
  "version": "0.1.4",
  "repository": "UMN-LATIS/latis-vue-components",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": {
    "name": "James Johnson",
    "email": "johnsojr@umn.edu"
  },
  "scripts": {
    "dev": "npm run storybook",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "rm -rf dist-storybook && build-storybook -o dist-storybook -s public",
    "build": "rm -rf dist && rollup -c",
    "prepublishOnly": "npm run build",
    "release": "release-it",
    "gh-pages:build": "npm run build-storybook && touch dist-storybook/.nojekyll",
    "gh-pages:serve": "npx serve dist-storybook",
    "gh-pages:publish": "gh-pages --dotfiles --dist dist-storybook",
    "gh-pages": "npm run gh-pages:build && npm run gh-pages:publish",
    "yolo": "npm run release && npm run gh-pages"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/vue3": "^6.3.7",
    "@vue/compiler-sfc": "^3.2.6",
    "babel-loader": "^8.2.2",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.16.0",
    "gh-pages": "^3.2.3",
    "postcss": "^8.3.6",
    "release-it": "^14.11.5",
    "rollup": "^2.56.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "vue-loader": "^16.5.0"
  },
  "peerDependencies": {
    "vue": "^3.2.6"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/vue-fontawesome": "^3.0.0-4"
  },
  "publishConfig": {
    "access": "public"
  }
}
